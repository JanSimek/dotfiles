# enable the dock when 3.0 usb hub connected
ACTION=="add|remove", SUBSYSTEM=="usb", ATTRS{idVendor}=="2109", ATTRS{idProduct}=="0812", RUN+="/etc/udev/scripts/dock-action $env{ACTION}"
